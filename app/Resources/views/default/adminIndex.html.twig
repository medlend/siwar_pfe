{% extends 'baseAdmin.html.twig' %}

{% block body %}



        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">

                        <table class="table table-condensed table-hover">
                            <thead>
                            <tr>
                                <th class="span2"></th>
                                <th class="span9"></th>
                            </tr>
                            </thead>
                            <tbody>

                            {% for value in messages %}

                                <tr>
                                    <td><a href="{{ path('admin_contact', { 'id': value[2]}) }}" ><strong>{{ value[0] }}</strong></a></td>
                                    <td><strong>{{ value[1][1] }}</strong></td>
                                </tr>
                            {% endfor %}

                            </tbody>
                        </table>


                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->

{% endblock %}

